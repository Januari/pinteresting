<h1>Listing Pins</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @pinnings.each do |pinning| %>
      <tr>
        <td><%= pinning.description %></td>
        <td><%= link_to 'Show', pinning %></td>
        <td><%= link_to 'Edit', edit_pinning_path(pinning) %></td>
        <td><%= link_to 'Destroy', pinning, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pins', new_pinning_path %>
